<?xml version='1.0' encoding='utf-8'?>
<plugin id="thirdlogin" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>thirdlogin</name>
    <js-module name="thirdlogin" src="www/thirdlogin.js">
        <clobbers target="cordova.plugins.thirdlogin" />
    </js-module>

    <!--添加我们自己的平台  -->
    <platform name="android">  <!-- 平台类型 -->
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="ThirdLogin">  <!-- JS调用的对象名 window.android -->
                <param name="android-package" value="com.z.thirdlogin.ThirdLogin"/> <!-- .java类名全路径 -->
            </feature>
        </config-file>
        <source-file src="src/android/ThirdLogin.java" target-dir="src/com/z/thirdlogin" />
        <!-- src:java源文件的路径， target-dir:插件安装好后，源文件的位置，要和上面的包名对应 -->
    </platform>
</plugin>
